{
  "name": "@wistia/wistia-api-client",
  "version": "2026.1.1",
  "author": "Speakeasy",
  "type": "module",
  "tshy": {
    "sourceDialects": [
      "@wistia/wistia-api-client/source"
    ],
    "exports": {
      ".": "./src/index.ts",
      "./package.json": "./package.json",
      "./types": "./src/types/index.ts",
      "./models/errors": "./src/models/errors/index.ts",
      "./models": "./src/models/index.ts",
      "./models/operations": "./src/models/operations/index.ts",
      "./*.js": "./src/*.ts",
      "./*": "./src/*.ts"
    }
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/wistia/wistia-typescript-sdk.git"
  },
  "scripts": {
    "lint": "eslint --cache --max-warnings=0 src",
    "build": "tshy",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.0.0",
    "dotenv": "^17.2.2",
    "eslint": "^9.19.0",
    "globals": "^15.14.0",
    "tshy": "^2.0.0",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.26.0",
    "uuid": "^13.0.0"
  },
  "dependencies": {
    "zod": "^3.25.0 || ^4.0.0"
  },
  "exports": {
    ".": {
      "import": {
        "@wistia/wistia-api-client/source": "./src/index.ts",
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/commonjs/index.d.ts",
        "default": "./dist/commonjs/index.js"
      }
    },
    "./package.json": "./package.json",
    "./types": {
      "import": {
        "@wistia/wistia-api-client/source": "./src/types/index.ts",
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/types/index.js"
      },
      "require": {
        "types": "./dist/commonjs/types/index.d.ts",
        "default": "./dist/commonjs/types/index.js"
      }
    },
    "./models/errors": {
      "import": {
        "@wistia/wistia-api-client/source": "./src/models/errors/index.ts",
        "types": "./dist/esm/models/errors/index.d.ts",
        "default": "./dist/esm/models/errors/index.js"
      },
      "require": {
        "types": "./dist/commonjs/models/errors/index.d.ts",
        "default": "./dist/commonjs/models/errors/index.js"
      }
    },
    "./models": {
      "import": {
        "@wistia/wistia-api-client/source": "./src/models/index.ts",
        "types": "./dist/esm/models/index.d.ts",
        "default": "./dist/esm/models/index.js"
      },
      "require": {
        "types": "./dist/commonjs/models/index.d.ts",
        "default": "./dist/commonjs/models/index.js"
      }
    },
    "./models/operations": {
      "import": {
        "@wistia/wistia-api-client/source": "./src/models/operations/index.ts",
        "types": "./dist/esm/models/operations/index.d.ts",
        "default": "./dist/esm/models/operations/index.js"
      },
      "require": {
        "types": "./dist/commonjs/models/operations/index.d.ts",
        "default": "./dist/commonjs/models/operations/index.js"
      }
    },
    "./*.js": {
      "import": {
        "@wistia/wistia-api-client/source": "./src/*.ts",
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "require": {
        "types": "./dist/commonjs/*.d.ts",
        "default": "./dist/commonjs/*.js"
      }
    },
    "./*": {
      "import": {
        "@wistia/wistia-api-client/source": "./src/*.ts",
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "require": {
        "types": "./dist/commonjs/*.d.ts",
        "default": "./dist/commonjs/*.js"
      }
    }
  },
  "main": "./dist/commonjs/index.js",
  "types": "./dist/commonjs/index.d.ts",
  "module": "./dist/esm/index.js"
}
